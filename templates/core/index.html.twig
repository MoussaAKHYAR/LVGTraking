

{% extends 'base.html.twig' %}

{% block title %}Tableau de bord{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-md-12">
            <div class="card card-transparent">
                <div class="card-header">
                    <h4 class="card-title text-center">
                        Position des vehicules en temps reel
                    </h4>
                </div>
                <div class="card-body">
                    <div class="col-md-12 ml-auto mr-auto">

                        <div id="allData" style="display: none">{{ allData|json_encode }}</div>


                        <div id="map" style="height: 400px"></div>
                        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB6SNzfHX80SmqcSBnIJz0FdB43nfcr5CM&callback=initMap"
                                async defer></script>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
